{
    "name": "yarcode/yii2-app-yarcode",
    "description": "Yii 2 YarCode Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "https://github.com/yarcode/yii2-app-yarcode",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yarcode/yii2-app-yarcode/issues?state=open",
        "source": "https://github.com/yarcode/yii2-app-yarcode"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.7",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yarcode/yii2-base": "^0.3.1",
        "kartik-v/yii2-widget-alert": "^1.1",
        "kartik-v/yii2-widget-growl": "^1.1",
        "yarcode/yii2-fa": "^1.0",
        "dmstr/yii2-adminlte-asset": "2.*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
